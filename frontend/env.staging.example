# Staging Environment Configuration
# Copy this file to .env.staging and update with your actual values

# Application
VITE_APP_ENVIRONMENT=staging
VITE_APP_DEBUG=false
VITE_LOG_LEVEL=info

# API Configuration
VITE_API_BASE_URL=https://staging-api.novora.com
VITE_API_VERSION=v1

# Feature Flags
VITE_FEATURE_AUTO_PILOT=true
VITE_FEATURE_ADVANCED_ANALYTICS=true
VITE_FEATURE_SSO=true
VITE_FEATURE_API_KEYS=true
VITE_FEATURE_WEBHOOKS=true
VITE_FEATURE_REAL_TIME_UPDATES=true

# Staging Settings
VITE_DEV_PROXY=false
VITE_DEV_HOT_RELOAD=false
VITE_DEV_SOURCE_MAPS=true

# External Services (Staging)
VITE_ANALYTICS_URL=https://staging-analytics.novora.com
VITE_MONITORING_URL=https://staging-monitoring.novora.com
VITE_SUPPORT_URL=https://staging-support.novora.com

# Security (Staging)
VITE_ENABLE_HTTPS=true
VITE_ENABLE_CORS=true
VITE_SESSION_TIMEOUT=3600000
VITE_MAX_LOGIN_ATTEMPTS=5

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
VITE_ENABLE_PWA=false
