{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://novora-backend.onrender.com",
    "VITE_MIN_N": "5",
    "VITE_DEFAULT_LANG": "es"
  }
}
