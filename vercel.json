{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "installCommand": "echo 'No install needed'",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dashboard(.*)",
      "dest": "/maintenance.html"
    },
    {
      "src": "/api/(?!health)(.*)",
      "dest": "/maintenance.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}