# Development Environment Configuration
# Copy this file to .env.development and update with your actual values

# Application
VITE_APP_ENVIRONMENT=development
VITE_APP_DEBUG=true
VITE_LOG_LEVEL=debug

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_VERSION=v1

# Feature Flags
VITE_FEATURE_AUTO_PILOT=true
VITE_FEATURE_ADVANCED_ANALYTICS=true
VITE_FEATURE_SSO=false
VITE_FEATURE_API_KEYS=false
VITE_FEATURE_WEBHOOKS=false
VITE_FEATURE_REAL_TIME_UPDATES=false

# Development Settings
VITE_DEV_PROXY=true
VITE_DEV_HOT_RELOAD=true
VITE_DEV_SOURCE_MAPS=true

# External Services (Development)
VITE_ANALYTICS_URL=
VITE_MONITORING_URL=
VITE_SUPPORT_URL=

# Security (Development)
VITE_ENABLE_HTTPS=false
VITE_ENABLE_CORS=true
VITE_SESSION_TIMEOUT=3600000
VITE_MAX_LOGIN_ATTEMPTS=5
